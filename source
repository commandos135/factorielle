#include "stdafx.h"
#include <iostream> 
using namespace std;

int f(int n)
{
	if (n == 0)
		return 1;
	else return n*f(n - 1);
}

int main()
{
	int n;
	/////////////////////////////////////////
	cout << " please enter n: " << endl;
	cin >> n;	
	
	
	cout << f(n) << endl;
	

	system("pause");
	return 0;
}
